services:
  catalog.api:
    build:
      context: F:\ASP.NET_CORE\micro\aspnet-microservices\aspnet-microservices
      dockerfile: Catalog.API/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: catalogapi
    ports:
    - target: 80
  catalogdb:
    image: mongo
version: '3.4'
volumes:
  mongo_data: {}